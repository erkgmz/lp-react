{
  "name": "development",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch sass:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "sass": "sass src/css/sass/input.scss:src/css/output.css",
    "sass:watch": "npm run sass -- --watch",
    "add-sitemap": "cp sitemap.xml ./dist/static/",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir -p dist/static/",
    "compile-server": "babel tools/distServer.js --out-file dist/server.js",
    "compile-server-routes": "mkdir dist/routes/ && babel tools/routes/contactRoute.js --out-file dist/routes/contactRoute.js",
    "compile-server-controllers": "mkdir dist/controllers/ && babel tools/controllers/contactController.js --out-file dist/controllers/contactController.js",
    "build-es5-server": "npm-run-all compile-server compile-server-routes compile-server-controllers",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist add-sitemap build:html build-es5-server",
    "build": "babel-node tools/build.js",
    "postbuild": "node dist/server.js"
  },
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "body-parser": "^1.15.2",
    "cheerio": "^0.22.0",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "dotenv": "^2.0.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "flightplan": "^0.6.15",
    "html-loader": "^0.4.4",
    "node-sass": "^3.13.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "1.8.0",
    "postcss-loader": "^1.1.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "2.5.2",
    "react-styleable": "^2.2.4",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "sendgrid": "^4.7.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "colors": "1.1.2",
    "connect-history-api-fallback": "^1.3.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "html-webpack-plugin": "^2.28.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "rimraf": "^2.5.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "webpack-md5-hash": "0.0.5"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "5.7.1"
  }
}
